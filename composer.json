{
    "name": "grpc/php",
    "description": "GRPC PHP",
    "repositories": [
        {
            "type": "path",
            "url": "../.."
        }
    ],
    "require": {
        "google/protobuf": "^3.19",
        "openswoole/grpc": "^22.1.5",
        "openswoole/core": "^22.1.5",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Grpcphp\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "symfony/var-dumper": "^6.4"
    }
}
